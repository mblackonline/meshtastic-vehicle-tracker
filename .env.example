# Copy to .env and set strong values for your environment

# PostgreSQL Configuration
POSTGRES_DB=fleet_tracker
POSTGRES_USER=fleet_user
POSTGRES_PASSWORD=change_me
TZ=UTC

# Database host - use "postgres" when running collector in Docker (default)
# or "127.0.0.1" when running collector locally in venv
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# MQTT Broker Configuration
# For device configs (WisMesh gateway): Use your server's LAN IP
MQTT_HOST=192.168.x.x  # Change to your server's IP address (e.g., 192.168.3.205)
MQTT_PORT=1883
MESHTASTIC_MQTT_ROOT=msh/fleet
# Note: Docker collector auto-connects to "mosquitto" service via docker-compose override

# Meshtastic Device Configuration
WIFI_SSID=your_wifi_ssid
WIFI_PASSWORD=your_wifi_password
# Treat as secret (channel + PSK). Do not commit real values.
CHANNEL_URL=replace_with_private_channel_url
